    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "use-sync-external-store": "^1.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "main": "dist/reactodia-workspace.js",
  "typings": "dist/typings/index.d.ts",
  "files": [
    "dist/reactodia-workspace.js",

    "typings": "tsc --project tsconfig.base.json --declaration --emitDeclarationOnly --outDir dist/typings",
    "lint": "eslint --ext .jx,.jsx,.ts,.tsx src/ examples/",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "classnames": "^2.3.2",

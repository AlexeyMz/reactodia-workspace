{
  "name": "@reactodia/workspace",
  "version": "0.20.0",
  "description": "Reactodia Workspace -- library for visual interaction with graphs in a form of a diagram.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AlexeyMz/reactodia-workspace.git"
  },
  "keywords": [
    "owl",
    "graph",
    "rdf",
    "diagram",
    "sparql"
  ],
  "author": {
    "name": "Alexey Morozov",
    "url": "https://github.com/AlexeyMz"
  },
  "license": "LGPL-2.1",
  "bugs": {
    "url": "https://github.com/AlexeyMz/reactodia-workspace/issues"
  },
  "homepage": "https://github.com/AlexeyMz/reactodia-workspace",
  "scripts": {
    "build": "webpack",
    "build-examples": "webpack --config webpack.examples.config.js",
    "serve": "webpack-dev-server --config webpack.examples.config.js --open",
    "test": "npm run build && npm run build-examples",
    "typings": "tsc --project tsconfig.base.json --declaration --emitDeclarationOnly --outDir dist/typings",
    "lint": "tslint -c tslint.ci.json --project tsconfig.json --exclude '**/*.d.ts' -t stylish",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "d3-color": "^3.1.0",
    "file-saver": "^2.0.5",
    "n3": "^1.17.2",
    "webcola": "~3.3.8"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18",
    "react-dom": "^17.0.2 || ^18"
  },
  "devDependencies": {
    "@rdfjs/types": "^1.1.0",
    "@types/d3-color": "^3.1.2",
    "@types/file-saver": "^2.0.6",
    "@types/n3": "^1.16.3",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "main": "dist/reactodia-workspace.js",
  "typings": "dist/typings/index.d.ts"
}

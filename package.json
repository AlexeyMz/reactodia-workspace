    "webcola": "~3.3.8"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18",
    "react-dom": "^17.0.2 || ^18"
  },
  "devDependencies": {
    "@rdfjs/types": "^1.1.0",
    "@types/d3-color": "^3.1.2",
    "@types/file-saver": "^2.0.6",
    "@types/n3": "^1.16.3",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/use-sync-external-store": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vscode/codicons": "^0.0.35",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
